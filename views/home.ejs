            // Function to show sections
            function showSection(section) {
                const dynamicDiv = document.getElementById('dynamicContent');
                dynamicDiv.classList.remove('fade-in');
                void dynamicDiv.offsetWidth;
                dynamicDiv.innerHTML = dynamicSections[section];
                dynamicDiv.classList.add('fade-in');
            }

            function showSection(section) {
                const dynamicDiv = document.getElementById('dynamicContent');
                dynamicDiv.classList.remove('fade-in'); // reset animation
                void dynamicDiv.offsetWidth; // trigger reflow
                dynamicDiv.innerHTML = dynamicSections[section];
                dynamicDiv.classList.add('fade-in');
            }

            function jumpToSlide(index) {
                const carouselEl = document.getElementById('propertyCarousel');
                const carousel = bootstrap.Carousel.getInstance(carouselEl) || new bootstrap.Carousel(carouselEl);
                carousel.to(index);
            }


