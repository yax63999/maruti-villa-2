            <h2 class="text-primary mb-2">3 & 4 BHK Duplex Bungalows</h2>
            <p class="text-muted mb-4">in Valsad, Gujarat</p>

            <div class="row justify-content-center g-3">

                <!-- Plot Area -->
                <div class="col-md-3 col-6">
                    <div class="info-box p-4 rounded shadow-sm bg-light h-100">
                        <i class="fa-solid fa-map fa-2x text-primary mb-2"></i>
                        <p class="mb-0 fw-semibold">Plot Area</p>
                        <small class="text-muted">1700 / 2000 sq.ft</small>
                    </div>
                </div>

                <!-- Furnishing -->
                <div class="col-md-3 col-6">
                    <div class="info-box p-4 rounded shadow-sm bg-light h-100">
                        <i class="fa-solid fa-couch fa-2x text-primary mb-2"></i>
                        <p class="mb-0 fw-semibold">Furnishing</p>
                        <small class="text-muted">Semi Furnished</small>
                    </div>
                </div>

                <!-- Overlooking -->
                <div class="col-md-3 col-6">
                    <div class="info-box p-4 rounded shadow-sm bg-light h-100">
                        <i class="fa-solid fa-tree fa-2x text-primary mb-2"></i>
                        <p class="mb-0 fw-semibold">Overlooking</p>
                        <small class="text-muted">Park / Garden</small>
                    </div>
                </div>

                <!-- Possession Starts -->
